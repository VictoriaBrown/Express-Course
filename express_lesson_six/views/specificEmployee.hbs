<h1>{{LastName}}, {{FirstName}}</h1>
<h3>Title: {{Title}}</h3>
<h3>Reports To: {{ReportsTo}}</h3>
<h3>DOB: {{BirthDate}}</h3>
<h3>Hire Date: {{HireDate}}</h3>
<h3>Address: {{Address}}</h3>
<h3>City: {{City}}</h3>
<h3>State: {{State}}</h3>
<h3>Fax: {{Fax}}</h3>
<h3>Email: {{Email}}</h3>

<script>
    function onDelete() {
        $.ajax({
            url: '/employees/{{EmployeeId}}/delete',
            method: 'DELETE', complete: () => window.location = '/employees'
        })
    }
</script>

<div>
    <button onclick="onDelete()" id="deleteButton">Delete Employee</button>
</div>